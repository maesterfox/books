{
  "builds": [
    {
      "src": "build",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist",
        "buildCommand": "REACT_APP_BACKEND_URL=$REACT_APP_BACKEND_URL npm run build"
      }
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "@backend-url"
  }
}
